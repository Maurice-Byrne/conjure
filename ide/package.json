{
    "name": "conjure",
    "displayName": "CONJURE",
    "description": "",
    "version": "0.0.1",
    "publisher": "revilotom",
    "engines": {
        "vscode": "^1.27.0"
    },
    "main": "./out/extension",
    "categories": [
        "Programming Languages",
        "Formatters"
    ],
    "activationEvents": [
        "onLanguage:essence"
    ],
    "contributes": {
        "commands": [
            {
                "command": "conjure.model",
                "title": "model essence",
                "category": "Conjure"
            },
            {
                "command": "conjure.solve",
                "title": "solve essence",
                "category": "Conjure"
            },
            {
                "command": "conjure.vis",
                "title": "Visualise a search tree",
                "category": "Conjure"
            }
        ],
        "languages": [
            {
                "id": "essence",
                "aliases": [
                    "Essence",
                    "essence"
                ],
                "extensions": [
                    ".essence",
                    ".eprime",
                    ".param",
                    ".essence-param",
                    ".eprime-param",
                    ".solution",
                    ".eprime-solution",
                    ".essence-solution"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "essence",
                "scopeName": "source.essence",
                "path": "./syntaxes/Essence.tmLanguage"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^8.10.36",
        "gulp": "^4.0.0",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "webpack": "^4.25.1",
        "webpack-cli": "^3.1.2"
    },
    "dependencies": {
        "@types/mathjs": "^4.4.1",
        "create-html": "^4.1.0",
        "d3-3": "0.0.0",
        "d3-node": "^2.1.0",
        "deep-rename-keys": "^0.2.1",
        "is-empty": "^1.2.0",
        "jsondiffpatch": "^0.3.11",
        "mathjs": "^5.2.1",
        "node-stopwatch": "0.0.1",
        "request": "^2.88.0",
        "sqlite3": "^4.0.4",
        "vinyl-source-stream": "^2.0.0"
    }
}
