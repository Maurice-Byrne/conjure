language ESSENCE' 1.0

find x: int(1..9)
find y: int(1..9)
find z: int(1..9)
find s_ExplicitVarSizeWithFlags_Flags: matrix indexed by [int(1..9)] of bool
find s_ExplicitVarSizeWithFlags_Values: matrix indexed by [int(1..9)] of int(1..9)
find u: int(1..9)
branching on [x, y, z, s_ExplicitVarSizeWithFlags_Flags, s_ExplicitVarSizeWithFlags_Values, u]
such that
    x + y = z,
    allDiff([x, y, z; int(1..3)]),
    x % 2 = 1,
    z % 2 = 1,
    sum([toInt(s_ExplicitVarSizeWithFlags_Flags[q6]) * catchUndef(s_ExplicitVarSizeWithFlags_Values[q6], 0)
             | q6 : int(1..9)])
    = x + y + z,
    (u - x) * z = 9,
    and([s_ExplicitVarSizeWithFlags_Flags[q1 + 1] ->
         s_ExplicitVarSizeWithFlags_Values[q1] < s_ExplicitVarSizeWithFlags_Values[q1 + 1]
             | q1 : int(1..8)]),
    and([s_ExplicitVarSizeWithFlags_Flags[q2] = false -> s_ExplicitVarSizeWithFlags_Values[q2] = 1 | q2 : int(1..9)]),
    and([s_ExplicitVarSizeWithFlags_Flags[q3 + 1] -> s_ExplicitVarSizeWithFlags_Flags[q3] | q3 : int(1..8)])

